<div id="detailsWrapper">
  <h2 id="reservationDetailsTitle">{{title}}</h2>

  <div class="contentWrapper">
    <label class="reservationDetailsLabel">Reservation ID: </label><span class="reservationContent">{{id}}</span>
  </div>

  <div class="contentWrapper">
    <label class="reservationDetailsLabel">Room ID: </label><span class="reservationContent">{{roomId}}</span>
  </div>

  <div class="contentWrapper">
    <label class="reservationDetailsLabel">Start: </label><span id="eventDetailStart" class="reservationContent">{{start}}</span>
  </div>

  <div class="contentWrapper">
    <label class="reservationDetailsLabel">End: </label><span id="eventDetailEnd" class="reservationContent">{{end}}</span>
  </div>

  <div class="contentWrapper">
    <label class="reservationDetailsLabel">Duration: </label><span id="reservationDuration" class="reservationContent"></span>
  </div>

  <button id="returnToCalendar" class="btn btn-lg">Return to calendar</button>
</div>
